{
  "name": "text-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "codemirror": "^5.38.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-froala-wysiwyg": "^2.8.1",
    "react-scripts": "1.1.4",
    "xml": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "froala",
    "froala-editor",
    "froala-wysiwyg",
    "html",
    "text",
    "editor",
    "wysiwyg",
    "jquery-plugin",
    "rich editor",
    "rich text editor",
    "rte",
    "javascript",
    "jquery"
  ],
  "filename": "js/froala_editor.min.js",
  "main": "js/froala_editor.min.js",
  "npmName": "froala-editor",
  "npmFileMap": [
    {
      "basePath": "/",
      "files": [
        "js/plugins/*.min.js",
        "js/third_party/*.min.js",
        "js/froala_editor.min.js",
        "js/froala_editor.pkgd.min.js",
        "css/froala_editor.min.css",
        "css/froala_editor.pkgd.min.css",
        "css/froala_style.min.css",
        "css/plugins/*.min.css",
        "css/third_party/*.min.css",
        "css/themes/*.min.css"
      ]
    }
  ]
}
